/* pages/msg/msg.wxss */
.container{background-color: #ffffff;display: flex;flex-direction: column;}
.list-item{width: 690rpx;min-height: 120rpx;background-color: #fcfaf5;margin: 0 auto 28rpx auto;text-align: left;display: flex;justify-content: center;align-items: center;border-radius: 10rpx;box-shadow: 2px 2px 2px 2px rgba(204,204,204,0.5);flex-direction: column;}
.msg-list{height: 910rpx;margin-top: 40rpx;}
.msg-title{font-size: 24rpx;color:#464646;margin-left: 20rpx;margin-top: 20rpx;}
.time{display: flex;justify-content: space-between;width: 100%;}
.msg-time{color:#999999;font-size: 20rpx;width: 220rpx;margin: 20rpx;}
.msg-empty{width: 100rpx;}
.msg-foot{width: 750rpx;height: 202rpx;position: absolute;left: 0;bottom: 0;}
.msg-img{width: 253rpx;height: 216rpx;position: absolute;right: 0;bottom: 0;}
