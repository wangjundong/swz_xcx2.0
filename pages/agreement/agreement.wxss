/* pages/agreement/agreement.wxss */
page{width: 100%;height: 100%;background-color: #a0d8ef;}
.content{display: flex;justify-content: center;align-items: center;flex-direction: column;padding: 0;margin: 0;}
.main{width: 690rpx;height: 1000rpx;background-color: #ffffff;color:#333333;font-size: 28rpx;text-align: justify;border-radius: 20rpx;display: inline-block;padding: 0 20rpx;}
.main text{display: block;margin: 15rpx 0;}
.read{display: flex;color:#ffffff;font-size: 28rpx;margin-top: 20rpx;}
.read checkbox{height: 30rpx;width:30rpx;margin-right: 28rpx;line-height: 30rpx;}
.agree{width: 690rpx;height: 80rpx;background-color: #2bb2e8;font-size: 36rpx;color:#ffffff;line-height: 80rpx;margin-top: 28rpx;}
